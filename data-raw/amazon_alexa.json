{
    "@context": "https://schema.org/",
    "@type": "Dataset",
    "name": "Amazon Alexa Reviews",
    "schemaVersion": "Psych-DS 0.1.0",
    "author": "Lisa DeBruine",
    "description": "This dataset contains text reviews as well as the 1-5 rating from customers who bought an Alexa device on Amazon.",
    "license": "CC-BY 4.0",
    "variableMeasured": [
        {
            "@type": "PropertyValue",
            "name": "rating",
            "description": "The 1-5 rating",
            "dataType": "int"
        },
        {
            "@type": "PropertyValue",
            "name": "date",
            "description": "The date of the rating",
            "dataType": "string"
        },
        {
            "@type": "PropertyValue",
            "name": "variation",
            "description": "The type of Alexa",
            "dataType": "string"
        },
        {
            "@type": "PropertyValue",
            "name": "feedback",
            "description": "The text feedback",
            "dataType": "string"
        },
        {
            "@type": "PropertyValue",
            "name": "verified_reviews",
            "description": "Whether the review was verified (1) or not (0)",
            "dataType": "int"
        }
    ]
}
